<p id="notice"><%= notice %></p>

<h1>Users table (Admin access) </h1>


<table class="table table-striped table-bordered table-hower">
  <thead class="thead-dark">
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th>Vaccine status</th>
      <th>Aadhaar</th>
      <th>Email ID</th>
      <th>Phone no</th>
      <th>Address</th>
      <th>Pin code</th>
      <th>VaccineTaken</th>
      
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.password %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.gender %></td>
        <td><%= user.vaccine_status %></td>
        <td><%= user.aadhaar %></td>
        <td><%= user.email_id %></td>
        <td><%= user.phone_no %></td>
        <td><%= user.address %></td>
        <td><%= user.pin_code %></td>
        <td><%= user.noofvacc %></td>
        
        
      </tr>
    <% end %>
  </tbody>
</table>